		<servlet-name>bill</servlet-name>
		<url-pattern>/bill.go</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>delete</servlet-name>
		<servlet-class>edu.wctc.my.kgable.controller.DeleteController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>delete</servlet-name>
		<url-pattern>/delete.go</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>search</servlet-name>
		<servlet-class>edu.wctc.my.kgable.controller.SearchController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>search</servlet-name>
		<url-pattern>/search.go</url-pattern>
	</servlet-mapping>
</web-app>