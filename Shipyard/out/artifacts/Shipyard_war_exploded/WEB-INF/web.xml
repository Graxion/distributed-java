	    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	    version="4.0">
	<servlet>
		<servlet-name>catalog</servlet-name>
		<servlet-class>edu.wctc.my.kgable.controller.CatalogController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>catalog</servlet-name>
		<url-pattern>/catalog.go</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>cart</servlet-name>
		<servlet-class>edu.wctc.my.kgable.controller.CartController</servlet-class>
