		<url-pattern>/processcart.go</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>bill</servlet-name>
		<servlet-class>edu.wctc.my.kgable.controller.BillController</servlet-class>
	</servlet>
